{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-useless-escape": 0,
      "no-console": 0,
      "func-names": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
